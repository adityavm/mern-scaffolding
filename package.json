  "main": "app.js",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "forever": "^0.15.3",
    "helmet": "^3.1.0",
    "mocha": "^3.2.0",
    "mongodb": "^2.2.11",
    "node-sass": "^3.13.0",
    "q": "^1.4.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "devDependencies": {},
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "build": "./node_modules/.bin/webpack",
    "app": "./node_modules/.bin/forever start app.js",
    "dev": "./node_modules/.bin/forever start ./node_modules/.bin/webpack-dev-server --content-base dist/",
    "db": "mongod --config mongod.conf --fork --logpath logs/mongo.log",
    "start": "npm run-script app; npm run-script dev; npm run-script db;",
    "end": "./node_modules/.bin/forever stopall; printf '\n\\e[41mNOTE\\e[49m\\e[31m Manually kill MongoDB server\\e[90m (ps aux | grep mongod)\n'"
